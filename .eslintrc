{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "forOf": true,
        "jsx": true,
        "es6": true
    },
    "rules": {
        "no-use-before-define": 0,
        "indent": [2, 2, {"SwitchCase": 1}],
        "react/prop-types": 0,
        "new-cap": 0,
    }
}
